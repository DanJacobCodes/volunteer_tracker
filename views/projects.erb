<% if @projects.any?() %>
      <h4>All Projects: </h4>
      <ul>
      <% @projects.each() do |project| %>
        <li><a href="/projects/<%= project.id() %>"><%= project.name() %></a></li>
      <% end %>
      </ul>

  <% else %>
      <h2>No projects at this time</h2>
  <% end %>
</div>

<a href="/">Home</a>
