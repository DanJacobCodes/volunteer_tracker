<h1>View volunteers</h1>
<a href="/">Home</a>

<div class="container">

  <% if @volunteers.any?() %>
      <h3>Volunteers: </h3>
      <ul>
      <% @volunteers.each() do |volunteer| %>
        <li><a href="/volunteers/<%= volunteer.id() %>"><%= volunteer.name() %></a></li>
      <% end %>
      </ul>
  <% else %>
      <h2>No volunteers at this time</h2>
  <% end %>
</div>
